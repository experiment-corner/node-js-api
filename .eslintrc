{
  "extends": "fortech",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "no-console": 1,
    "jsx-a11y/img-has-alt": 0,
    "arrow-body-style": 0,
    "prefer-template": 0,
    "no-underscore-dangle": 0,
    "jsx-a11y/href-no-hash": 0,
    "react/forbid-prop-types": [ 1, { "forbid": [] }],
    "jsx-a11y/no-noninteractive-element-interactions": 0,
    "linebreak-style": 0
  }
}
